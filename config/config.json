{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "welcome-user-msf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/welcomeUserMsf/WelcomeUserMsfWebPart.js",
          "manifest": "./src/webparts/welcomeUserMsf/WelcomeUserMsfWebPart.manifest.json"
        }
      ]
    },
    "button-msf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/buttonMsf/ButtonMsfWebPart.js",
          "manifest": "./src/webparts/buttonMsf/ButtonMsfWebPart.manifest.json"
        }
      ]
    },
    "tabs-msf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/tabsMsf/TabsMsfWebPart.js",
          "manifest": "./src/webparts/tabsMsf/TabsMsfWebPart.manifest.json"
        }
      ]
    },
    "anchor-list-msf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/anchorListMsf/AnchorListMsfWebPart.js",
          "manifest": "./src/webparts/anchorListMsf/AnchorListMsfWebPart.manifest.json"
        }
      ]
    },
    "dropzone-msf-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/dropzoneMsf/DropzoneMsfWebPart.js",
          "manifest": "./src/webparts/dropzoneMsf/DropzoneMsfWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "tinymce": {
      "path": "https://cdnjs.cloudflare.com/ajax/libs/tinymce/5.10.3/tinymce.min.js",
      "globalName": "tinymce"
    }
  },
  "localizedResources": {
    "WelcomeUserMsfWebPartStrings": "lib/webparts/welcomeUserMsf/loc/{locale}.js",
    "ButtonMsfWebPartStrings": "lib/webparts/buttonMsf/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js",
    "TabsMsfWebPartStrings": "lib/webparts/tabsMsf/loc/{locale}.js",
    "AnchorListMsfWebPartStrings": "lib/webparts/anchorListMsf/loc/{locale}.js",
    "DropzoneMsfWebPartStrings": "lib/webparts/dropzoneMsf/loc/{locale}.js"
  }
}
