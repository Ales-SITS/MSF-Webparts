{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "825ee057-8325-4e55-a4a7-6e159f646232",
  "alias": "RedirectMsfWebPart",
  "componentType": "WebPart",

  "version": "*",
  "manifestVersion": 2,

  
  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart", "TeamsPersonalApp", "TeamsTab", "SharePointFullPage"],
  "supportsThemeVariants": true,

  "preconfiguredEntries": [{
    "groupId": "5c03119e-3074-46fd-976b-c60198311f70", 
    "group": { "default": "Advanced" },
    "title": { "default": "Redirect (MSF)" },
    "description": { "default": "This webpart helps you to create an automatic redirect from pange to any other page or site you choose. You can also set redirect delay, message and more." },
    "iconImageUrl": "data:image/png;base64,",
    "properties": {
      "redirect_message_l": "This is an old site. You will be redirected to ",
      "redirect_message_link": "this site,",
      "redirect_message_r": "in",
      "redirect_counter": 5,
      "redirect_BG": "#ffffff",
      "redirect_BR": 0,
      "redirect_size": 24,
      "redirect_FC": "#ff0000",

      "redirect_counter_display" : true,
      "redirect_counter_size": 60,
      "redirect_counter_FC": "#000000",

      "redirect_stop_display": true,
      "redirect_stop_text" : "Stay on site",
      "redirect_stop_size": 14,
      "redirect_stop_BG": "#d1d1d1",
      "redirect_stop_BR": 50,
      "redirect_stop_FC": "#000000"
    }
  }]
}

  